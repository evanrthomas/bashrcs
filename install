#! /bin/bash
#create required directories if they doen't exist
curdir=$(pwd)
mkdir -p ~/scripts
mkdir -p ~/.marks

if [ -f ~/.gat ]; then
  git clone https://github.com/zingales/gat.git ~/.gat
  cd ~/.gat;mv ex_gat_script ~/scripts/gat
else
  cd ~/.gat;git pull
fi
cd $curdir

python install.py
#move all files except profile to home dir

./git_config.sh
